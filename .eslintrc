{
  "extends": [ "esnext" ],
  "env": {
    "browser": true
  },
  "rules": {
    "no-console": [ "error", { "allow": [ "log", "dir", "warn", "error" ] } ],
    "semi": [ "error", "never" ],
    "space-before-function-paren": [ "error", "always" ],
    "space-infix-ops": [ "error", { "int32Hint": true } ]
  }
}
